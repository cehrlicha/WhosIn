 <%= link_to event_path(e) do %>
    <div class="horizontal-card card-img" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('http://res.cloudinary.com/cehrlicha/image/upload/v1496073649/bbq_atwwbi.jpg');">
     <div class="card-date"><%= Date::DAYNAMES[e.start_date.wday] %> <%= e.start_date.to_date  %></div>
        <div class="card-description">
          <h2><%= e.title %></h2>
        </div>
    </div>
  <% end %>
